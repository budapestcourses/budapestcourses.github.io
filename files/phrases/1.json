  "813": {
    "id": "813",
    "text": "给我们智慧。"
  },
  "814": {
    "id": "814",
    "text": "给我们力量去做所有你要求我们做的事。"
  },
  "815": {
    "id": "815",
    "text": "求（/请求）XXX"
  },
  "816": {
    "id": "816",
    "text": "我们求 …"
  },
  "817": {
    "id": "817",
    "text": "我们求您在这个困难的时候指引我们。"
  },
  "818": {
    "id": "818",
    "text": "祝福XXX"
  },
  "819": {
    "id": "819",
    "text": "祝福 …"
  },
  "820": {
    "id": "820",
    "text": "祝福我们。"
  },
  "821": {
    "id": "821",
    "text": "祝福我们的传道工作。"
  },
  "822": {
    "id": "822",
    "text": "祝福这个聚会。"
  },
  "823": {
    "id": "823",
    "text": "祝福我们的会众。"
  },
  "824": {
    "id": "824",
    "text": "保护XXX"
  },
  "825": {
    "id": "825",
    "text": "保护我们。"
  },
  "826": {
    "id": "826",
    "text": "保护那些今天晚上不能来这儿的弟兄。"
  },
  "827": {
    "id": "827",
    "text": "保护那些在别的地方受到迫害的弟兄。"
  },
  "828": {
    "id": "828",
    "text": "我们晚上回家的时候，（求你）保护我们。"
  },
  "829": {
    "id": "829",
    "text": "请 … ; 求你 …",
    "annotation": "（:祷告时使我们的请求更礼貌或语气更强烈的语词）"
  },
  "830": {
    "id": "830",
    "text": "祝福这个聚会。"
  },
  "831": {
    "id": "831",
    "text": "请祝福这个聚会。"
  },
  "832": {
    "id": "832",
    "text": "罪和难题"
  },
  "833": {
    "id": "833",
    "text": "我们是不完美的。"
  },
  "834": {
    "id": "834",
    "text": "犯罪"
  },
  "835": {
    "id": "835",
    "text": "我们多次犯罪。"
  },
  "836": {
    "id": "836",
    "text": "对不起。；我们很抱歉。"
  },
  "837": {
    "id": "837",
    "text": "对不起（/我们很抱歉 ) , 我们（常常）犯罪。"
  },
  "838": {
    "id": "838",
    "text": "对不起，我们是罪人。"
  },
  "839": {
    "id": "839",
    "text": "宽恕"
  },
  "840": {
    "id": "840",
    "text": "我们谦卑地请求您的宽恕。"
  },
  "841": {
    "id": "841",
    "text": "宽恕XXX （：XXX是一个人）"
  },
  "842": {
    "id": "842",
    "text": "请宽恕我们。"
  },
  "843": {
    "id": "843",
    "text": "请您因您的儿子付出的赎价而宽恕我们。"
  },
  "844": {
    "id": "844",
    "text": "宽恕XXX（：XXX是一种罪）"
  },
  "845": {
    "id": "845",
    "text": "请宽恕我们的罪。"
  },
  "846": {
    "id": "846",
    "text": "我们生活在一个邪恶的世界里。"
  },
  "847": {
    "id": "847",
    "text": "我们经历很多困难（/难题）。"
  }
}