  "787": {
    "id": "787",
    "text": "感謝您派您的兒子來地球。"
  },
  "788": {
    "id": "788",
    "text": "感謝您通過聚會給我們的提醒。"
  },
  "789": {
    "id": "789",
    "text": "請求"
  },
  "790": {
    "id": "790",
    "text": "引導XXX"
  },
  "791": {
    "id": "791",
    "text": "引導我們。"
  },
  "792": {
    "id": "792",
    "text": "引導你的組織。"
  },
  "793": {
    "id": "793",
    "text": "引導中央長老團。"
  },
  "794": {
    "id": "794",
    "text": "幫助XXX"
  },
  "795": {
    "id": "795",
    "text": "幫助我們事奉您。"
  },
  "796": {
    "id": "796",
    "text": "幫助我們做對的事情。"
  },
  "797": {
    "id": "797",
    "text": "幫助我們抵抗引誘。"
  },
  "798": {
    "id": "798",
    "text": "幫助我們忍耐。"
  },
  "799": {
    "id": "799",
    "text": "幫助我們繼續參加聚會。"
  },
  "800": {
    "id": "800",
    "text": "幫助我們傳道時找到心地誠實的人。"
  },
  "801": {
    "id": "801",
    "text": "幫助我們告訴別人我們學到的知識。"
  },
  "802": {
    "id": "802",
    "text": "幫助我們聚會期間可以集中精神。"
  },
  "803": {
    "id": "803",
    "text": "幫助我們沉思你的話語。"
  },
  "804": {
    "id": "804",
    "text": "幫助我們把學到的知識應用出來。"
  },
  "805": {
    "id": "805",
    "text": "幫助那些現在有困難的弟兄。"
  },
  "806": {
    "id": "806",
    "text": "幫助我們留意到弟兄的需要。"
  },
  "807": {
    "id": "807",
    "text": "幫助XXX繼續學習你的話語。"
  },
  "808": {
    "id": "808",
    "text": "幫助XXX有勇氣。"
  },
  "809": {
    "id": "809",
    "text": "幫助XXX在屬靈方面進步。"
  },
  "810": {
    "id": "810",
    "text": "給YYY XXX"
  },
  "811": {
    "id": "811",
    "text": "給我們 …"
  },
  "812": {
    "id": "812",
    "text": "給我們你的聖靈。"
  },
  "813": {
    "id": "813",
    "text": "給我們智慧。"
  },
  "814": {
    "id": "814",
    "text": "給我們力量去做所有你要求我們做的事。"
  },
  "815": {
    "id": "815",
    "text": "求（/請求）XXX"
  },
  "816": {
    "id": "816",
    "text": "我們求 …"
  },
  "817": {
    "id": "817",
    "text": "我們求您在這個困難的時候指引我們。"
  },
  "818": {
    "id": "818",
    "text": "祝福XXX"
  },
  "819": {
    "id": "819",
    "text": "祝福 …"
  },
  "820": {
    "id": "820",
    "text": "祝福我們。"
  },
  "821": {
    "id": "821",
    "text": "祝福我們的傳道工作。"
  },
  "822": {
    "id": "822",
    "text": "祝福這個聚會。"
  },
  "823": {
    "id": "823",
    "text": "祝福我們的會眾。"
  },
  "824": {
    "id": "824",
    "text": "保護XXX"
  },
  "825": {
    "id": "825",
    "text": "保護我們。"
  },
  "826": {
    "id": "826",
    "text": "保護那些今天晚上不能來這兒的弟兄。"
  },
  "827": {
    "id": "827",
    "text": "保護那些在別的地方受到迫害的弟兄。"
  },
  "828": {
    "id": "828",
    "text": "我們晚上回家的時候，（求你）保護我們。"
  },
  "829": {
    "id": "829",
    "text": "請 … ; 求你 …",
    "annotation": "（:禱告時使我們的請求更禮貌或語氣更強烈的語詞）"
  },
  "830": {
    "id": "830",
    "text": "祝福這個聚會。"
  },
  "831": {
    "id": "831",
    "text": "請祝福這個聚會。"
  },
  "832": {
    "id": "832",
    "text": "罪和難題"
  },
  "833": {
    "id": "833",
    "text": "我們是不完美的。"
  },
  "834": {
    "id": "834",
    "text": "犯罪"
  },
  "835": {
    "id": "835",
    "text": "我們多次犯罪。"
  },
  "836": {
    "id": "836",
    "text": "對不起。；我們很抱歉。"
  },
  "837": {
    "id": "837",
    "text": "對不起（/我們很抱歉 ) , 我們（常常）犯罪。"
  },
  "838": {
    "id": "838",
    "text": "對不起，我們是罪人。"
  },
  "839": {
    "id": "839",
    "text": "寬恕"
  },
  "840": {
    "id": "840",
    "text": "我們謙卑地請求您的寬恕。"
  },
  "841": {
    "id": "841",
    "text": "寬恕XXX （：XXX是一個人）"
  },
  "842": {
    "id": "842",
    "text": "請寬恕我們。"
  },
  "843": {
    "id": "843",
    "text": "請您因您的兒子付出的贖價而寬恕我們。"
  },
  "844": {
    "id": "844",
    "text": "寬恕XXX（：XXX是一種罪）"
  },
  "845": {
    "id": "845",
    "text": "請寬恕我們的罪。"
  },
  "846": {
    "id": "846",
    "text": "我們生活在一個邪惡的世界裡。"
  },
  "847": {
    "id": "847",
    "text": "我們經歷很多困難（/難題）。"
  }
}